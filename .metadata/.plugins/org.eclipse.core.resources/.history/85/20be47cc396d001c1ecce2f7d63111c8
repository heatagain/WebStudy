	<%
		response.setContentType("text/html; charset=utf-8");
		String room = request.getParameter("room");
		String floor = request.getParameter("floor");
		int a = Integer.parseInt(room);
		int b = Integer.parseInt(floor);
		//지금 아래 쓰고있는 floor room은 받아온 숫자처럼 보이는 문자입니다
		int s = 1;
		for(int k = 0; k < a; k++){
			out.print("<tr>");
			for (int i = 0; i < b; i++) {
				out.print("<td>" + b + "</td>");
				
			}
			out.print("</tr>");
		}%>
		
